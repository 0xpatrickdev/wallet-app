{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "esnext",
    "module": "esnext",
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "downlevelIteration": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "moduleResolution": "node"
  },
  "include": [
    "./src"
  ]
}
